<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <template id="training_order_template" name="Training Order">
        <div class="training-order">
            <table class="table table-striped">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Order Number</th>
                    </tr>
                </thead>
                <tbody>
                    <t t-foreach="orders_data" t-as="order">
                        <tr>
                            <td>
                                <t t-esc="order.get('id')" />
                            </td>
                            <td>
                                <t t-esc="order.get('display_name')" />
                            </td>
                        </tr>
                    </t>
                </tbody>
            </table>
        </div>
    </template>
</odoo>